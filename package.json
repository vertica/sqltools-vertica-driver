{
  "name": "sqltools-vertica-driver",
  "displayName": "SQLTools Vertica driver",
  "description": "SQLTools Driver for Vertica",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "publisher": "Vertica",
  "license": "Apache",
  "repository": {
    "type": "git",
    "url": "https://github.com/vertica/sqltools-vertica-driver.git"
  },
  "bugs": {
    "url": "https://github.com/vertica/sqltools-vertica-driver/issues"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "keywords": [
    "vertica",
    "sql",
    "dbsql",
    "sqltools-driver",
    "data warehouse"
  ],
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters",
    "Other"
  ],
  "extensionDependencies": [
    "mtxr.sqltools"
  ],
  "activationEvents": [
    "*",
    "onLanguage:sql",
    "onCommand:sqltools.*"
  ],
  "main": "./out/extension.js",
  "dependencies": {
    "@sqltools/base-driver": "latest",
    "@sqltools/types": "latest",
    "tsc": "^2.0.4",
    "typecript": "^0.0.1-security",
    "uuid": "^7.0.3",
    "vertica-nodejs": "^1.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@types/node": "^14.0.9",
    "@types/vscode": "^1.42.0",
    "typescript": "^3.9.10"
  }
}
